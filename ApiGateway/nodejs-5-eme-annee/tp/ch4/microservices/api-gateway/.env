# Port où l'API Gateway s'exécute
PORT=3000

# URI de la base de données MongoDB
MONGO_URI=mongodb://localhost:27017/api-gateway-db

# Clé secrète pour JWT (utilisé pour l'authentification)
JWT_SECRET=your_jwt_secret_key

# Expiration des tokens JWT
JWT_EXPIRATION=1h

# Configuration CORS (si applicable)
CORS_ALLOWED_ORIGINS=http://localhost:4000,http://localhost:5000

# Configuration du logging (si applicable)
LOG_LEVEL=info

# Redis (si utilisé pour le cache)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# D'autres variables spécifiques aux services (si nécessaire)
SERVICE_A_PORT=4000
SERVICE_B_PORT=5000
